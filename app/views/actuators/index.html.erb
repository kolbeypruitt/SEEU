<h1>Listing actuators</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Manufacturer</th>
    <th>Model</th>
    <th>Data uri</th>
    <th>Configuration uri</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @actuators.each do |actuator| %>
  <tr>
    <td><%= actuator.name %></td>
    <td><%= actuator.description %></td>
    <td><%= actuator.manufacturer %></td>
    <td><%= actuator.model %></td>
    <td><%= link_to actuator.data_uri, actuator.data_uri %></td>
    <td><%= link_to actuator.configuration_uri, actuator.configuration_uri %></td>
    <td><%= link_to 'Show', actuator %></td>
    <td><%= link_to 'Edit', edit_actuator_path(actuator) %></td>
    <td><%= link_to 'Destroy', actuator, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

